<!-- SCROLL SNEAK JS: you can change `location.hostname` to any unique string or leave it as is -->

<script src="/javascripts/jquery.js"></script>
<script src="/javascripts/jquery.jplayer.min.js"></script>
<script>
(function() {
    var sneaky = new ScrollSneak(location.hostname), tabs = document.getElementsByTagName('nav'), i = 0, len = tabs.length;
    for (; i < len; i++) {
        tabs[i].onclick = sneaky.sneak;
    }
})();

$('.jp-jplayer').each(function(index){
  var href = $(this).attr('data-href');

  //SET PLAYER
  $('#jquery_jplayer_' + index).jPlayer({
    ready: function () {
      $(this).jPlayer("setMedia", {
        mp3: href
      });
    },
    play: function() { // To avoid multiple jPlayers playing together.
      $(this).jPlayer("pauseOthers");
    },
    swfPath: "/javascripts/Jplayer.swf",
    cssSelectorAncestor: '#jp_container_' + index,
    supplied: "mp3",
    wmode: "window",
    smoothPlayBar: true,
    keyEnabled: true
  });
});
</script>
